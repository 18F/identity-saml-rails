exclude_paths:
  - db/migrate
  - spec/**/*

IrresponsibleModule:
  enabled: false
TooManyStatements:
  max_statements: 6

'spec':
  DuplicateMethodCall:
    enabled: false
  FeatureEnvy:
    enabled: false
  NestedIterators:
    exclude:
      - build_configs
  TooManyStatements:
    enabled: false
